oject);\r\n                localStorage.setItem(`${index}`, data);\r\n                index++;\r\n            }\r\n        }\r\n    };\r\n\r\n    render() {\r\n        const { projectList } = this.state;\r\n        this.storeDataToLocalStorage(projectList);\r\n        console.dir(projectList); //FIXME: This runs twice per render, find out why and potentially fix it\r\n        return (\r\n            <div className=\"container\">\r\n                {projectList !== null && projectList.length ? (\r\n                    <div className=\"wrapper\">\r\n                        {projectList.map((item, index) => {\r\n                            return (\r\n                                <Card\r\n                                    key={index}\r\n                                    to={index}\r\n                                    data={JSON.stringify(item)}\r\n                                    title={item.title}\r\n                                    content={item.content}\r\n                                />\r\n                            );\r\n                        })}\r\n                    </div>\r\n                ) : (\r\n                    <div className=\"wrapper\">\r\n                        <Card />\r\n                        <Card />\r\n                        <Card />\r\n                        <Card />\r\n                        <Card />\r\n                        <Card />\r\n                        <Card />\r\n                        <Card />\r\n                    </div>\r\n                )}\r\n            </div>\r\n        );\r\n    }\r\n}\r\nclass Blog extends Component {\r\n    render() {\r\n        return (\r\n            <div id=\"Body\">\r\n                <NavLink className=\"right-align\" to={`/blog/new`}>\r\n                    <button className=\"btn right-align\">Create new Post</button>\r\n                </NavLink>\r\n                <Outlet />\r\n            </div>\r\n        );\r\n    }\r\n}\r\nclass Login extends Component {\r\n    constructor() {\r\n        super();\r\n        this.state = {\r\n            user: '',\r\n            password: '',\r\n        };\r\n    }\r\n\r\n    handleUser = (event) => {\r\n        this.setState({\r\n            user: event.target.value,\r\n        });\r\n        console.log(event.target.value);\r\n    };\r\n\r\n    handlePassword = (event) => {\r\n        this.setState({\r\n            password: event.target.value,\r\n        });\r\n        console.log(event.target.value);\r\n    };\r\n\r\n    handleSubmit = (event) => {\r\n        alert(`Server: ${this.state.user}`);\r\n        event.preventDefault();\r\n    };\r\n\r\n    render() {\r\n        return (\r\n            <div id=\"Body\">\r\n                <form id=\"loginForm\" onSubmit={this.handleSubmit}>\r\n                    <input\r\n                        type=\"text\"\r\n                        value={this.state.user}\r\n                        onChange={this.handleUser}\r\n                        placeholder=\"@user\"\r\n                    />\r\n                    <input\r\n                        type=\"password\"\r\n                        value={this.state.password}\r\n                        onChange={this.handlePassword}\r\n                        placeholder=\"********\"\r\n                    />\r\n                    <input type=\"submit\" value=\"Submit\" className=\"button\" />\r\n                </form>\r\n            </div>\r\n        );\r\n    }\r\n}\r\nclass SignUp extends Component {\r\n    render() {\r\n        return (\r\n            <div>\r\n                <div id=\"Body\">Sign Up</div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nReactDOM.render(\r\n    <React.StrictMode>\r\n        <Router>\r\n            <Header />\r\n            <Routes>\r\n                <Route path=\"/\" element={<Home />} />\r\n                <Route path=\"/projects\" element={<Projects />}>\r\n                    <Route path=\"\" element={<ProjectList />} />\r\n                    <Route path=\